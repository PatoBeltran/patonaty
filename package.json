{
  "name": "patonaty",
  "version": "0.0.1",
  "description": "RSVP page for wedding",
  "main": "index.js",
  "homepage": "http://PatoBeltran.github.io/patonaty",
  "scripts": {
    "start": "webpack-dev-server --hot --colors --open --host 0.0.0.0",
    "build:watch": "webpack --mode=development --config webpack.config.js --watch",
    "build-prod:watch": "webpack --mode=production --config webpack.config.js --watch",
    "build": "webpack --mode=development --config webpack.config.js",
    "build-prod": "webpack --mode=production --config webpack.config.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "trello": "node ./onTrelloUpdate.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "i18next": "^15.0.4",
    "i18next-browser-languagedetector": "^3.0.0",
    "i18next-xhr-backend": "^2.0.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-i18next": "^10.1.2",
    "react-page-scroller": "^1.4.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^3.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.18",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^24.0.5",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "jest": "^24.1.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "setupFiles": [
      "./testconfig/test-shim.js",
      "./testconfig/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|png|gif|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "./testconfig/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
